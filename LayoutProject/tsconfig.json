{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "outDir": "app",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    //"noStrictGenericChecks": true,
    "skipLibCheck": true,
    "lib": [
      "es6",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
